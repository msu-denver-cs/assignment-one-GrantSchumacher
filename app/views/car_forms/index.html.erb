<p id="notice"><%= notice %></p>

<h1>Car Forms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_forms.each do |car_form| %>
      <tr>
        <td><%= link_to 'Show', car_form %></td>
        <td><%= link_to 'Edit', edit_car_form_path(car_form) %></td>
        <td><%= link_to 'Destroy', car_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Form', new_car_form_path %>
